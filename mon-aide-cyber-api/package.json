{
  "name": "mon-aide-cyber-api",
  "version": "0.0.0",
  "description": "API de MonAideCyber",
  "scripts": {
    "build": "knex migrate:latest --knexfile src/infrastructure/entrepots/postgres/knexfile.ts --migrations-directory migrations",
    "start": "ts-node --transpile-only --require dotenv/config index.ts",
    "dev": "knex migrate:latest --knexfile src/infrastructure/entrepots/postgres/knexfile.ts --migrations-directory migrations && nodemon --require dotenv/config index.ts",
    "administration:rattrapage-relations-entites-aidees-diagnostic": "ts-node --transpile-only --require dotenv/config src/administration/diagnostics/rattrapage-entites-diagnostics/commande.ts",
    "administration:rattrapage-journalisation-excel-demo": "ts-node --transpile-only --require dotenv/config src/administration/journal/rattrapage/commande.ts",
    "administration:rattrapage-des-fantomes": "ts-node --transpile-only --require dotenv/config src/administration/journal/rattrapage-des-fantomes/commande.ts",
    "administration:activation-compte-aidant": "ts-node --transpile-only --require dotenv/config src/administration/gestion-demandes/activation-compte/commande.ts",
    "administration:modification-emails-de-comptes": "ts-node --transpile-only --require dotenv/config src/administration/utilisateurs/modification-emails-de-comptes/commande.ts",
    "administration:recreer-compte-authentification": "ts-node --transpile-only --require dotenv/config src/administration/utilisateurs/recreer-compte-authentification/commande.ts",
    "administration:mise-en-relation-aidants": "ts-node --transpile-only --require dotenv/config src/administration/aidants/mise-en-relation/commande.ts",
    "administration:relance-affectation-demande-aide": "ts-node --transpile-only --require dotenv/config src/administration/aidants/relance-mise-en-relation/commande.ts",
    "administration:rapport-demandes-aidants": "ts-node --transpile-only --require dotenv/config src/administration/gestion-demandes/demandes-en-cours/commande.ts",
    "administration:recherche-demandes-aidants": "ts-node --transpile-only --require dotenv/config src/administration/gestion-demandes/recherche/commande.ts",
    "administration:recherche-aidants": "ts-node --transpile-only --require dotenv/config src/administration/aidants/recherche/commande.ts",
    "administration:recherche-utilisateurs": "ts-node --transpile-only --require dotenv/config src/administration/utilisateurs/recherche/commande.ts",
    "administration:suppression-demandes-devenir-aidant": "ts-node --transpile-only --require dotenv/config src/administration/gestion-demandes/suppression-demandes-devenir-aidant/commande.ts",
    "administration:suppression-comptes-aidant": "ts-node --transpile-only --require dotenv/config src/administration/aidants/suppression-de-comptes/commande.ts",
    "administration:extraction-aidants": "ts-node --transpile-only --require dotenv/config src/administration/aidants/extraction-aidants/commande.ts",
    "administration:recupere-aidants-par-domaine": "ts-node --transpile-only --require dotenv/config src/administration/aidants/par-domaine/commande.ts",
    "administration:rattrapage-gendarmes": "ts-node --transpile-only --require dotenv/config src/administration/gendarmes/rattrapage/commande.ts",
    "administration:chiffrement": "ts-node --transpile-only --require dotenv/config src/administration/chiffrement/commande.ts",
    "administration:lie-diagnostics": "ts-node --transpile-only --require dotenv/config src/administration/lie-diagnostics/commande.ts",
    "administration:challenge-mars": "ts-node --transpile-only --require dotenv/config src/administration/aidants/challenge-mars/commande.ts",
    "ci:initialise": "ts-node --transpile-only --require dotenv/config test/infrastructure/entrepots/postgres/integration_continue.ts",
    "tache:rapport-cot-hebdomadaire": "ts-node --transpile-only --require dotenv/config src/infrastructure/taches/tacheExtractionCOT.ts ",
    "test": "tsc --noEmit && vitest run --config vitest.config.ts",
    "test:coverage": "vitest --config vitest.config.ts run --coverage",
    "test:coverage:ui": "vitest --config vitest.config.ts --ui --coverage --reporter verbose",
    "test:watch": "vitest --config vitest.config.ts --ui --reporter verbose"
  },
  "dependencies": {
    "@lab-anssi/lib": "^1.3.1",
    "@sentry/node": "^7.76.0",
    "@types/cookie-session": "^2.0.47",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lusca": "^1.7.4",
    "body-parser": ">=1.20.3",
    "commander": "^11.1.0",
    "cookie-session": "^2.0.0",
    "date-fns": "^3.2.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-ipfilter": "^1.3.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "lusca": "^1.7.0",
    "openid-client": "^5.7.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "pug": "^3.0.3",
    "puppeteer": "^24.10.0",
    "ts-node": "^10.9.1",
    "yaml": "^2.7.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.199",
    "@types/pg": "^8.10.5",
    "@types/pug": "^2.0.6",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.43.0",
    "light-my-request": "^5.11.0",
    "lodash.clonedeep": "^4.5.0",
    "nodemon": "^2.0.22"
  }
}
