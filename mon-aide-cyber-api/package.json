{
  "name": "mon-aide-cyber-api",
  "version": "0.0.0",
  "description": "API de MonAideCyber",
  "scripts": {
    "build": "",
    "start": "ts-node --transpile-only --require dotenv/config index.ts",
    "dev": "nodemon --require dotenv/config index.ts",
    "test": "tsc && vitest run --config vitest.config.ts --reporter verbose",
    "test:coverage": "vitest --config vitest.config.ts run --coverage",
    "test:watch": "vitest --config vitest.config.ts --ui --reporter verbose"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "pdf-lib": "^1.17.1",
    "pug": "^3.0.2",
    "puppeteer": "^21.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/pug": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "vitest": "^0.32.2"
  }
}
